{
    "manifest_version":3,
    "name":"tabs Opener",
    "version":"1.0",
    "description":"open tabs with shortcats",
    "permissions":["commands"],
    "background":{
        "service_worker":"background.js"
    },
    "commands":{
        "open-whatsapp":{
            "suggested_key":{
                "default":"Alt+W"
            },
            "description":"open whatsapp"
        },
        "open-youtube":{
            "suggested_key":{
                "default":"Alt+Y"
            },
            "description":"open youtube"
        },
        "open-twitter":{
            "suggested_key":{
                "default":"Alt+T"
            },
            "description":"open twitter"
        },
        "open-chat":{
            "suggested_key":{
                "default":"Alt+C"
            },
            "description":"open chat"
        },
        "open-fb":{
            "suggested_key":{
                "default":"Alt+F"
            },
            "description":"open fb"
        }
    }
}